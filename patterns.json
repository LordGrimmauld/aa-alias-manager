[
  {
    "name": "bin",
    "target": "/bin",
    "store_suffixes": [
      "bin",
      "libexec",
      "sbin",
      "usr/bin",
      "usr/sbin"
    ],
    "individual": true,
    "only_exe": true,
    "disallowed_strings": [
      "!"
    ]
  },
  {
    "name": "lib",
    "target": "/lib",
    "store_suffixes": [
      "lib",
      "libexec",
      "lib32",
      "lib64",
      "usr/lib64",
      "usr/lib32",
      "usr/libexec"
    ]
  }
]
